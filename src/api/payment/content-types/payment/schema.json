{
  "kind": "collectionType",
  "collectionName": "payments",
  "info": {
    "singularName": "payment",
    "pluralName": "payments",
    "displayName": "Payment"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Amount": {
      "type": "decimal",
      "required": true
    },
    "payment_status": {
      "type": "enumeration",
      "required": true,
      "default": "processing",
      "enum": [
        "processing",
        "succeeded",
        "failed"
      ]
    },
    "PaymentMethod": {
      "type": "enumeration",
      "required": true,
      "default": "Cash",
      "enum": [
        "Cash",
        "Card"
      ]
    },
    "order": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::order.order",
      "inversedBy": "payment"
    }
  }
}
